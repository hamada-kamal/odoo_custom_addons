<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="receive_vehicle_popup" model="ir.ui.view">
            <field name="name">receive vehicle popup</field>
            <field name="model">receive.vehicle.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <h3>Enter Last Odometer Value ???</h3>
                    <group>
                        <field name="odometer" placeholder="K.M Out"/>
                    </group>
                    <footer>
                        <button name="set_odometer_out" type="object" string="confirm"
                                class="oe_highlight"/>
                        <button special="cancel" string="Cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>